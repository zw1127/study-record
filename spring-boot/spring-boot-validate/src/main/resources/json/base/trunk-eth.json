{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "nodeId": {
      "type": "string",
      "minLength": 1
    },
    "ethifIndex": {
      "type": "string",
      "pattern": "^\\d+$"
    },
    "ethIfTrunkPort": {
      "type": [
        "string",
        "null"
      ],
      "pattern": "^\\d+$"
    },
    "ethIfTrunkEnable": {
      "type": "boolean"
    }
  },
  "required": [
    "nodeId",
    "ethifIndex",
    "ethIfTrunkPort",
    "ethIfTrunkEnable"
  ]
}
