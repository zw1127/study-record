{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "nodeId": {
      "type": "string",
      "minLength": 1
    },
    "flapDetectEnable": {
      "type": "boolean"
    },
    "flapAgeTime": {
      "type": "integer",
      "minimum": 60,
      "maximum": 900
    },
    "flapQuitVlanRecoverTime": {
      "type": "integer",
      "minimum": 0,
      "maximum": 1440
    },
    "flappingDetectionExcludeVlan": {
      "type": "string"
    }
  },
  "required": [
    "nodeId",
    "macAgeTime"
  ]
}
