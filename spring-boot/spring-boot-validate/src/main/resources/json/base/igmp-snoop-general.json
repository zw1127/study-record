{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "nodeId": {
      "type": "string",
      "minLength": 1
    },
    "igmpSnoopEnable": {
      "type": "boolean"
    },
    "igmpRobustCoefficient": {
      "type": "integer",
      "minimum": 2,
      "maximum": 5
    },
    "igmpV2RouterAging": {
      "type": "integer",
      "minimum": 1,
      "maximum": 1000
    }
  },
  "required": [
    "nodeId",
    "igmpSnoopEnable",
    "igmpRobustCoefficient",
    "igmpV2RouterAging"
  ]
}
