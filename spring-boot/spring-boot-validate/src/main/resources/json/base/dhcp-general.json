{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "nodeId": {
      "type": "string",
      "minLength": 1
    },
    "dhcpEnable": {
      "type": "boolean"
    },
    "dhcpServerDetect": {
      "type": "boolean"
    },
    "dhcpTrackerInterval": {
      "type": "integer",
      "minimum": 60,
      "maximum": 3600
    }
  },
  "required": [
    "nodeId",
    "dhcpEnable",
    "dhcpServerDetect",
    "dhcpTrackerInterval"
  ]
}
