{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "nodeId": {
      "type": "string",
      "minLength": 1
    },
    "syncMode": {
      "type": "integer",
      "minimum": 1,
      "maximum": 8
    },
    "addressType": {
      "type": "string",
      "const": "ipv4"
    },
    "serverIp": {
      "type": "string",
      "format": "ipv4"
    },
    "serviceVersion": {
      "type": "integer",
      "minimum": 1,
      "maximum": 4
    },
    "authId": {
      "type": "integer",
      "minimum": 1,
      "maximum": 4294967295
    }
  },
  "required": [
    "nodeId",
    "syncMode",
    "addressType",
    "serverIp",
    "serviceVersion",
    "authId"
  ]
}
