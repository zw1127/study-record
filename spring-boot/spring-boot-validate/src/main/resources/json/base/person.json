{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "anyOf": [
        {"format": "ipv4"},
        {"format": "ipv6"}
      ],
      "minLength": 1
    },
    "age": {
      "type": "integer",
      "minimum": 18,
      "maximum": 45
    }
  },
  "required": [
    "name"
  ]
}
