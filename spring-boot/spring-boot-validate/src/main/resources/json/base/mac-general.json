{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "nodeId": {
      "type": "string",
      "minLength": 1
    },
    "macAgeTime": {
      "type": "integer",
      "oneOf": [
        {
          "const": 0
        },
        {
          "minimum": 60,
          "maximum": 1000000
        }
      ]
    }
  },
  "required": [
    "nodeId",
    "macAgeTime"
  ]
}
